docker_run {
    Name: "build_zip",
    Image: "/shanhu.io/lib/dockers/nodejs",
    Command: ["/bin/bash", "build.sh"],
    MountWorkspace: "/work",
    Input: {
        "build.sh": "/opt/node/build.sh",
    },
    Output: {
        "jarvis.zip": "/opt/node/drv-ts/jarvis.zip",
    },
    Deps: ["src"],
}

file_set {
    Name: "src",
    Select: ["**"],
}
